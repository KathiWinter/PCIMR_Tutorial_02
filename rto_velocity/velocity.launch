<launch>

    <node pkg="rto_velocity" type="velocity_node.py" name="velocity_node" ns="velocity" output="screen">
 
        <param name="stop_distance" value="0.33" />
        <param name="attention_distance" value="3" />

    </node>

    <node pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" name="teleop" output="screen">
        <remap from="cmd_vel" to="input/cmd_vel"/>
    </node>

    <include file="rto_simulation/rto_bringup_sim/launch/robot.launch"/> 

</launch>